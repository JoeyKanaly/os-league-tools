    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-charts": "^3.0.0-beta.54",
    "react-checkbox-tree": "^1.7.2",
    "react-device-detect": "^2.2.3",
    "react-dnd": "^14.0.5",
